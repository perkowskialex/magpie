<%- include('../partials/header') %>
<table id="list">
        <thead>
          <tr>
            <th>User</th>
            <th>Income</th>
            <th>Expenses</th>
          </tr>
        </thead>
        <tbody>
          <% budgets.forEach(function(b) { %>
            <tr>
              <td><%= b.name %></td>
              <td><%= b.income %></td>
              <td><a href="/budgets/<%= b._id %>"> <button class="btn waves-effect waves-light" type="submit" name="action">View
                    <i class="material-icons right">send</i>
                  </button></a></td>
            </tr>
            <% }); %>
          </tbody>
        </table>
        <button class="btn waves-effect waves-light" type="submit" name="action">Add Expense
                <i class="material-icons right">send</i>

<%- include('../partials/footer') %>